{
  "name": "shopify-example",
  "version": "1.0.8",
  "description": "Testing Shopify",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "cd semantic && gulp build",
    "start:server": "gulp copy && node index.js",
    "watch:server": "gulp copy && nodemon index.js",
    "build:css": "node-sass-chokidar src/ -o src/",
    "watch:css": "npm run build-css && node-sass-chokidar src/styles -o src/styles --watch --recursive",
    "start:app": "react-scripts start",
    "build:app": "react-scripts build",
    "test:app": "react-scripts test --env=jsdom",
    "eject:app": "react-scripts eject",
    "build": "gulp build",
    "watch": "gulp watch",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylevv/shopify-example.git"
  },
  "keywords": [
    "shopify"
  ],
  "author": "kylevv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kylevv/shopify-example/issues"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-run": "^1.7.1",
    "gulp-watch": "^5.0.0",
    "nodemon": "^1.14.12",
    "react-scripts": "^1.1.0"
  },
  "dependencies": {
    "@shopify/buy-button-js": "^0.5.1",
    "bcrypt-nodejs": "^0.0.3",
    "bcrypt-nodejs-as-promised": "^1.0.4",
    "body-parser": "^1.18.2",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-flash": "^0.0.2",
    "express-flash-messages": "^0.1.1",
    "express-session": "^1.15.6",
    "jquery": "^3.3.1",
    "mongoose": "^5.0.3",
    "morgan": "^1.9.0",
    "node-sass-chokidar": "0.0.3",
    "nonce": "^1.0.4",
    "pug": "^2.0.1",
    "raw-body": "^2.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "semantic-ui": "^2.3.0",
    "shopify-buy": "^1.0.2"
  }
}
